{
  "name": "news-portal",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --no-sourcemap && npm run minify_bundle && npm run minify_inline && npm run minify_polyfills&& npm run minify_vendor",
    "minify_bundle": "uglifyjs dist/inline.bundle.js --compress --mangle --output dist/inline.bundle.js",
    "minify_inline": "uglifyjs dist/main.bundle.js --compress --mangle --output dist/main.bundle.js",
    "minify_polyfills": "uglifyjs dist/polyfills.bundle.js --compress --mangle --output dist/polyfills.bundle.js",
    "minify_vendor": "uglifyjs dist/vendor.bundle.js --compress --mangle --output dist/vendor.bundle.js"
  },
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/material": "^2.0.0-beta.10",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@ngx-translate/core": "^7.2.0",
    "@ngx-translate/http-loader": "^1.0.2",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.2",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "typescript": "~2.3.3",
    "uglify-js": "^3.0.28"
  }
}
